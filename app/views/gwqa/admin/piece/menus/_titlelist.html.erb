<table class="index" cellspacing="0" cellpadding="0">
<tbody>
<% for i in @start .. @end %>
  <%item = @items[i - 1]%>
  <tr>
  <%if @current == i %>
    <td class="titleSelect"><%=hbr(item.title)%>(<%=item.latest_updated_at.strftime("%y-%m-%d")%>)</td>
  <%else%>
    <td class="title"><%=link_to(hbr(item.title) + "(#{item.latest_updated_at.strftime("%y-%m-%d")})", item.show_path + "&pp=#{i}" + gwbbs_params_set)%></td>
  <%end%>
  </tr>
<% end %>
</tbody>
</table>
