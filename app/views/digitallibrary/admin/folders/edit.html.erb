<%= render :partial => 'folder_name' %>
<%= form_for :item, :url => digitallibrary_folder_path(@item,{:title_id=>@title.id, :cat=>@item.parent_id}) ,:method=>:put, :html => {:multipart => true, :id => 'itemForm'} do |f| %>
 <%= f.error_messages %>
 <%= render :partial => 'form', :locals => {:f => f} %>
