<div class="bbsDocList circular">
<div class="pieceHeader"><h2 class="pieceTitle"><%=@system_title%></h2></div>

<div class="pieceBody">
<%= render :partial => 'header' %>

<%= form_for :item, :url => "/questionnaire/#{@item.id}" ,:html => {:method=>:put , :multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>
<div class="submitters"><div align="center">
<table><tr><td><%= submit_for(f, :js_submitter=>"my_submit('#{@item.state}')",:caption=>'保存') %></td></tr></table>
</div></div>

<% end %>
</div>
</div>
