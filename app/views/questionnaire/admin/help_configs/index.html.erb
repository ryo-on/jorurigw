<div class="bbsDocList circular">
<div class="pieceHeader">
<h2 class="pieceTitle"><%=@system_title%></h2>
<div class="btsHeaderLeft">
<span class="btNew"></span>
</div>
<div class="btsHeadeRight">
<span class="btSet"></span>
</div>
</div><!--pieceHeader-->

<div class="pieceBody">

<%= form_for :item, :url => questionnaire_help_config_path(@item) ,:html => {:method=>:put , :multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<div class="submitters"><%=f.submit("保存")%></div>

<% end %>

<!--戻るボタン-->
<div class="btReturn">
  <%=link_to "管理設定へ戻る", "/gw/config_settings" %>
</div>
</div><!--pieceBody-->
</div><!--piece gwbbs-->
