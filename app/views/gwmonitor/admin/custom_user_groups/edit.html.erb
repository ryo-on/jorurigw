<div class="bbsDocList circular setting">
<div class="pieceHeader">
<h2 class="pieceTitle"><%=@system_title%> 配信先グループ個人設定</h2>
</div><!--pieceHeader-->
<!--カラム用タグ-->
<div class="pieceBanner">
</div>
<div class="pieceBody">

<%= form_for :item, :url => @item.update_path ,:html => {:method=>:put , :multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>
<%= render :partial => 'form', :locals => {:f => f} %>

<div class="submitters">
<td><%= submit_for_update(f, :js_submitter => "my_submit()",:caption=>'登録する') -%></td>
</div>

<% end %>

</div><!--pieceBody-->
</div><!--piece gwbbs-->
