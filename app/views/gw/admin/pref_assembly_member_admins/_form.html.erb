    <tr><th>議員表示順<%= required %></th><td><%= f.text_field :u_order  , :size=>30, :class=>'IMEoff'%></td></tr>
    <tr><th>氏名<%= required %></th><td>姓：<%= f.text_field :u_lname , :size=>50 %></td></tr>
    <tr><th></th><td>名：<%= f.text_field :u_name , :size=>50 %></td></tr>
  </table>

<div class="preserve">
<%= submit_for(f, {:caption => '保存'}) %>
</div>
