<%= f.hidden_field :state   -%>
<%= f.error_messages %>
<%= required_head %>
  <table class="show">
    <tr><th>状態<%= required %></th><td><%= f.radio_buttons(:state ,Gw::PrefAssemblyMember.state_select, :class=>'state') %></td></tr>
    <tr><th>会派表示順<%= required %></th><td><%= f.text_field :g_order , :size=>30, :class=>'IMEoff' %></td></tr>
