<%# app/views/gwsub/public/sb04/sb04_seating_lists/_form %>
<%
cats = Gwsub::Sb01Training.select_cats(nil)
mark_jp = Gw::YearFiscalJp.select_dd_markjp_id(nil,5)
%>
<%= f.hidden_field :fyear_markjp %>
<%= hidden_field_tag 'v', @v %>
<%= hidden_field_tag 'limit', @limits %>
<%= hidden_field_tag 'fyed_id', @fyed_id %>
<%= hidden_field_tag 'pre_fyear', @fyed_id %>
<%= f.error_messages %>
<%= raw(required_head) %>
<table class="show">
<tr><th><%= raw(required) %>掲示板分類</th>
<td><%= f.select(:categories, options_for_select(cats ,@cat.to_i) ) %></td>
</tr>
<tr><th><%= raw(required) %>年度</th>
<td><%= f.select(:fyear_id, options_for_select(mark_jp ,@fyed_id) ) %></td>
</tr>
<tr><th><%= raw(required) %>見出し</th>
<td><%= f.text_field :title ,:size=>80 %></td>
</tr>
<tr><th><%= raw(required) %>掲示板URL</th>
<td><%= f.text_field :bbs_url ,:size=>80 %></td>
</tr>
<tr><th>備考</th>
<td><%= f.text_field :body ,:size=>80 %></td>
</tr>
<tr><th>状況</th>
<td><%= radio f,:state,Gwsub::Sb01Training.state_list, :value=>@item.state , :class=>'state' %></td>
</tr>
</table>
<br />
<div class="preserve">
<%= f.submit '保存' %>
</div>
<br />
<%#= render :partial => 'gwsub/public/sb04/sb04_common_info_form', :locals => {:f => f} %>
