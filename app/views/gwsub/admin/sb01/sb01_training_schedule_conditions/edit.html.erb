<br/>
<%= form_for :item, :url => @edit_path ,:action=>:update, :html => {:method => :put, :multipart => true, :class=>'leftSide'} do |f| %>
  <%= render :partial => 'form_edit', :locals => {:f => f} %>
<% end %>
  </div>
<div class="btReturn">
  <%= link_to '戻る', url_for("/gwsub/sb01/sb01_training_schedule_conditions/#{@item.id}?t_id=#{@item1.id}") %>
</div>
</div>