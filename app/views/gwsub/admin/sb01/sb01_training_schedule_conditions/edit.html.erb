<%
@edit_path = "#{@public_uri}/#{@item.id}?t_id=#{@item.training_id}"
@action = "edit"
-%>
<div class="gwsub sb01">
<div class="pieceHeader">
<h2>研修申込・受付</h2>
</div>
<div class="pieceBanner">
<%= render :partial => 'gwsub/admin/sb01/sb01_training_plans/menu' %>
</div>
<div class="pieceBody">
<%= render :partial => 'gwsub/admin/sb01/sb01_training_schedule_conditions/form_training_show' %>
