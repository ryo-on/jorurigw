<div class="gwsub sb04">
<%= render :partial => 'gwsub/admin/sb04/sb04_piece_header' %>
<div class="pieceBanner">
<%= render :partial => 'gwsub/admin/sb04/sb04_subfunction_select_menu' %>
<%= render :partial => 'gwsub/admin/sb04/sb04_level3menu_header' %>
<%= render :partial => 'gwsub/admin/sb04/sb04sections/display_name' %>
</div>
<div class="pieceBody">
<%= div_notice %>
  <table class="show">
    <tbody>
      <tr><th>年度</th><td><%=h @item.fyear_markjp -%></td></tr>
      <tr><th>所属コード</th><td><%=h @item.code -%></td></tr>
      <tr><th>所属名</th><td><%=h @item.name -%></td></tr>
      <tr><th>備考</th><td><%=h @item.remarks -%></td></tr>
      <tr><th>所属コード(システム)</th><td><%=h @item.ldap_code -%></td></tr>
      <tr><th>所属名(システム)</th><td><%=h @item.ldap_name -%></td></tr>
    </tbody>
  </table>
  <div class="btReturn">
  <%= link_to '一覧に戻る',"#{@csv_base_url}/index_csv?#{@qs}"   if params[:v]=='3' -%>
  <%= link_to '一覧に戻る',"#{@csv_base_url}/index3?#{@qs}"      if params[:v]=='4'  -%>
  </div>
</div>
</div>
