<div class="gwsub sb04">
  <%= render :partial => 'gwsub/admin/sb04/sb04_piece_header' %>
  <div class="pieceBanner">
  <%= render :partial => 'gwsub/admin/sb04/sb04_subfunction_select_menu' %>
  <%= render :partial => 'gwsub/admin/sb04/sb04_level3menu_header' %>
  <%= render :partial => 'gwsub/admin/sb04/sb04_editable_dates/display_name' %>
    <div class="showMenu">
    <%= link_to '編集' , "/gwsub/sb04/04/sb04_editable_dates/#{@item.id}/edit?#{@qs}" %>
    <%= link_to '削除'  ,"/gwsub/sb04/04/sb04_editable_dates/#{@item.id}?#{@qs}" ,:confirm=>"削除してよろしいですか？" ,:method=>:delete if @role_admin==true or @role_developer==true %>
    </div>
  </div>
  <div class="pieceBody">
  <%= div_notice %>

    <table class="show">
      <tbody>
        <tr>
          <th>年度</th>
          <td><%= @item.fyear_markjp -%></td>
        </tr>
        <tr>
          <th>編集開始日</th>
          <td><%= @item.start_at.strftime("%Y-%m-%d %H:%M:%S") if @item.start_at.present? -%></td>
        </tr>
        <tr>
          <th>編集終了日</th>
          <td><%= @item.end_at.strftime("%Y-%m-%d %H:%M:%S") if @item.end_at.present? -%></td>
        </tr>
        <tr>
          <th>見出し日付</th>
          <td><%= @item.headline_at.strftime("%Y-%m-%d") if @item.headline_at.present? -%></td>
        </tr>
      </tbody>
    </table>
    <div class="btReturn">
    <%= link_to '一覧に戻る',"/gwsub/sb04/04/sb04_editable_dates?#{@qs}" %>
    </div>
  </div>
</div>
